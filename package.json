{
  "name": "illvart.github.io",
  "version": "0.0.1",
  "description": "My Personal Website",
  "main": "n/a",
  "private": true,
  "author": {
    "name": "illvart",
    "email": "illvart@protonmail.com",
    "url": "https://twitter.com/illvart"
  },
  "license": "MIT",
  "homepage": "https://github.com/illvart/illvart.github.io#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/illvart/illvart.github.io.git"
  },
  "bugs": {
    "url": "https://github.com/illvart/illvart.github.io/issues"
  },
  "scripts": {
    "clean": "gulp clean",
    "dev": "yarn run format && cross-env NODE_ENV=development gulp dev",
    "build": "cross-env NODE_ENV=production gulp",
    "serve": "cross-env NODE_ENV=production gulp serve",
    "format": "prettier --check \"./**/*.{js,jsx,json,ts,tsx,css,html,yml,yaml,scss,sass}\" --write",
    "lint": "eslint \"./**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"./**/*.{js,jsx,ts,tsx}\"",
    "precommit": "yarn run format && yarn run lint:fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run precommit"
    }
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "bracketSpacing": false,
    "arrowParens": "always",
    "trailingComma": "all",
    "semi": true,
    "proseWrap": "always",
    "jsxBracketSameLine": true
  },
  "browserslist": [
    "last 2 versions",
    "> 0.25%",
    "IE > 10",
    "iOS >= 9",
    "Firefox ESR"
  ],
  "dependencies": {
    "browser-sync": "^2.26.13",
    "cross-env": "^7.0.2",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-babel-minify": "^0.5.1",
    "gulp-brotli": "^3.0.0",
    "gulp-data": "^1.3.1",
    "gulp-debug": "^4.0.0",
    "gulp-gray-matter": "^3.0.1",
    "gulp-html-minifier-terser": "^6.0.1",
    "gulp-nunjucks": "^5.1.0",
    "gulp-prettier": "^3.0.0",
    "gulp-pretty-data": "^0.1.2",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-sitemap": "^7.6.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-zopfli-green": "^5.0.1",
    "js-yaml": "^3.14.0",
    "marked": "^1.2.5",
    "modern-normalize": "^1.0.0",
    "nunjucks": "^3.2.2",
    "nunjucks-markdown": "^2.0.1",
    "prettier": "^2.2.1",
    "pretty-data": "^0.40.0",
    "quicklink": "^2.0.0",
    "workbox-build": "^6.0.0",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "eslint": "^7.14.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^6.0.1",
    "husky": "^4.3.0"
  }
}
